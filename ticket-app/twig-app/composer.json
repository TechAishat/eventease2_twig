{
    "name": "ticketmaster/app",
    "description": "A ticket management system built with PHP and Twig",
    "type": "project",
    "require": {
        "php": "^8.1",
        "twig/twig": "^3.0",
        "vlucas/phpdotenv": "^5.5",
        "illuminate/database": "^10.0",
        "illuminate/events": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
